  <ItemGroup>
	<None Update="app.config" />
    <None Update="FodyWeavers.xml" />
	<None Update="Properties\license.licx" />
  </ItemGroup>

  <ItemGroup>
	<EmbeddedResource Include="Highlighting\StackTrace.xshd" />
    <Page Include="**\*.xaml" SubType="Designer" Generator="MSBuild:Compile" Exclude="App.xaml" />
    <ApplicationDefinition Include="App.xaml" Generator="MsBuild:Compile" SubType="Designer" />	
	<Compile Update="App.xaml.cs" DependentUpon="App.xaml" />
    <Compile Update="**\*.xaml.cs" SubType="Designer" DependentUpon="%(Filename)" />
	<Compile Update="**\*.cs" SubType="Code" />
	<Resource Update="**\*.png" /> 
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\AssemblyInfo.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>AssemblyInfo.cs</LastGenOutput>
    </None>
    <Compile Update="Properties\AssemblyInfo.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AssemblyInfo.tt</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Service Include="{508349b6-6b84-4df5-91f0-309beebad82d}" />
  </ItemGroup>
  
  <Import Project="$(MSBuildSDKExtrasTargets)" Condition="Exists('$(MSBuildSDKExtrasTargets)')" />
</Project>