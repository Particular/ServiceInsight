    <PackageReference Include="Expression.Blend.Sdk" Version="1.0.2" />
    <PackageReference Include="Virtuosity.Fody" Version="1.20.0" />
	<PackageReference Include="PropertyChanged.Fody" Version="1.50.30" /> 
  </ItemGroup>
  <ItemGroup>
    <Reference Include="DevExpress.Data.v14.1" HintPath="..\..\lib\DevExpress\DevExpress.Data.v14.1.dll" />
    <Reference Include="DevExpress.Utils.v14.1" HintPath="..\..\lib\DevExpress\DevExpress.Utils.v14.1.dll" />
    <Reference Include="DevExpress.Xpf.Controls.v14.1" HintPath="..\..\lib\DevExpress\DevExpress.Xpf.Controls.v14.1.dll" />
