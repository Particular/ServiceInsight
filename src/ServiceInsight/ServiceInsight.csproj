    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.Entity.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Management" />
    <Reference Include="System.Messaging" />
    <Reference Include="System.Runtime.Caching" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Security" />
    <Reference Include="System.Transactions" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xaml" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="WindowsBase" />
  </ItemGroup>
  <ItemGroup> 
    <PackageReference Include="Autofac" Version="3.5.0.0" />
    <PackageReference Include="Autofac.Configuration" Version="3.3.0.0" />
    <PackageReference Include="Anotar.Serilog.Fody" Version="3.3.0" />
    <PackageReference Include="Caliburn.Micro" Version="2.0.2.0" />
    <PackageReference Include="Comparers" Version="4.0.0" />
    <PackageReference Include="EmptyConstructor.Fody" Version="1.2.1" />
    <PackageReference Include="gong-wpf-dragdrop" Version="0.1.4.1" />
    <PackageReference Include="Humanizer" Version="1.34.0" />
    <PackageReference Include="AvalonEdit" Version="5.0.2" />
    <PackageReference Include="Mindscape.Raygun4Net" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="6.0.8" />
